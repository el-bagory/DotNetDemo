@model Demo.Models.Department.DepartmentVM

@{
    ViewBag.Title = "Delete";
}

<h1>Delete Depertment</h1>

<form asp-action="Delete" asp-controller="Department" method="post" class="form-group">

    <label asp-for="DepartmentName">Department Name</label>
    <input type="text" asp-for="DepartmentName" disabled class="form-control" />
    
    <br/>

    <label asp-for="DepartmentCode">Department Code</label>
    <input type="text" asp-for="DepartmentCode" disabled class="form-control" />
    

    <br/>

    <center>
        <input type="submit" value="Delete" class="btn btn-primary" />
    </center>

   
</form>




<script src="~/jquery/jquery.min.js"></script>
<script src="~/jquery-validate/jquery.validate.js"></script>
<script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
