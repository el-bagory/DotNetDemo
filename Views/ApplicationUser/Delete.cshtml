@model Demo.Models.ApplicationUser.ApplicationUserVM

@{
    ViewBag.Title = "Delete Employee";
}
<link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet">

<h1>Delete Depertment</h1>

<form asp-action="Delete" asp-controller="ApplicationUser" method="post" class="form-group">

    <label asp-for="FullName">Name</label>
    <input disabled type="text" asp-for="FullName" class="form-control" />
    
    <br/>

    <label asp-for="Email">Email</label>
    <input disabled type="text" asp-for="Email" class="form-control" />
    

    <br />

    <center>
        <input type="submit" value="Delete" class="btn btn-primary" />
    </center>
</form>




<script src="~/jquery/jquery.min.js"></script>
<script src="~/jquery-validate/jquery.validate.js"></script>
<script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
