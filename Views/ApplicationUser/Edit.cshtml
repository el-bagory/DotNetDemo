@model Demo.Models.ApplicationUser.ApplicationUserVM

@{
    ViewBag.Title = "Edit Employee";
}
<link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet">

<h1>Edit Depertment</h1>

<form asp-action="Edit" asp-controller="ApplicationUser" method="post" class="form-group">

    <label asp-for="FullName">Full Name</label>
    <input type="text" asp-for="FullName" class="form-control" />
    <span class="text-danger" asp-validation-for="FullName"></span>
    <br/><br/>

    <label asp-for="Address">Address</label>
    <input type="text" asp-for="Address" class="form-control" />
    <span class="text-danger" asp-validation-for="Address"></span>
    <br/><br/>


    <label asp-for="Email">Email</label>
    <input type="text" asp-for="Email" class="form-control" />
    <span class="text-danger" asp-validation-for="Email"></span>

   <br /><br />
    


    <center>
        <input type="submit" value="Edit" class="btn btn-primary" />
    </center>
</form>




<script src="~/jquery/jquery.min.js"></script>
<script src="~/jquery-validate/jquery.validate.js"></script>
<script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
